<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<?import java.lang.String?>
<VBox alignment="CENTER" spacing="10" style="-fx-padding: 10;" styleClass="right-bar" stylesheets="@../../../sidebar.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.manula413.movie_manager.controller.SidebarController"
      prefWidth="100" maxWidth="100" minWidth="100" prefHeight="800" maxHeight="800" minHeight="800">
   <children>
      <Button fx:id="addMoviesNavButton" mnemonicParsing="false" onAction="#addMoviesNavButtonAction" prefHeight="41.0" stylesheets="@../../../style.css" text="Add Movies" textFill="WHITE">
         <styleClass>
            <String fx:value="right-bar-button" />
            <String fx:value="shadow" />
         </styleClass>
         <font>
            <Font name="Cambria Bold" size="16.0" />
         </font>
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="watchedListNavButton" mnemonicParsing="false" onAction="#watchedListNavButtonAction" prefHeight="41.0" stylesheets="@../../../style.css" text="Watched List" textFill="WHITE">
         <styleClass>
            <String fx:value="right-bar-button" />
            <String fx:value="shadow" />
         </styleClass>
         <font>
            <Font name="Cambria Bold" size="16.0" />
         </font>
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="watchLaterNavButton" mnemonicParsing="false" onAction="#watchLaterNavButtonAction" prefHeight="41.0" stylesheets="@../../../style.css" text="Watch Later" textFill="WHITE">
         <styleClass>
            <String fx:value="right-bar-button" />
            <String fx:value="shadow" />
         </styleClass>
         <font>
            <Font name="Cambria Bold" size="16.0" />
         </font>
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="extraButton" mnemonicParsing="false" prefHeight="41.0" stylesheets="@../../../style.css" text="Extra" textFill="WHITE">
         <styleClass>
            <String fx:value="right-bar-button" />
            <String fx:value="shadow" />
         </styleClass>
         <font>
            <Font name="Cambria Bold" size="16.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Button>
   </children>
</VBox>
